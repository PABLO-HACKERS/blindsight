{
	"QMainWindow":[
		{
			"tittle": "BlindSight",
			"icon": ":/images/logo.png",
			"frameless": true,
			"transluscentBg": true,
			"sizeGrip": "sizeGrip",
			"animationDuration": 1500,  
			"navigation":[
				{
					"minimize":"minimizeBtn",
					"close": "closeBtn",
					
					"restore":[
						{	
							"buttonName": "restoreBtn",
							"normalIcon": "icon/square.svg",
							"maximizedIcon": "icon/copy.svg"
						}
					],
					"moveWindow": "headerContainer",
					"tittleBar": "headerContainer"
				}
			]
		}
	],
